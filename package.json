{
  "name": "egg-logview",
  "version": "2.0.1",
  "description": "Provide a log files viewer for egg development purpose.",
  "eggPlugin": {
    "name": "logview",
    "env": [
      "local",
      "default",
      "test",
      "unittest"
    ]
  },
  "files": [
    "app",
    "lib",
    "config",
    "app.js"
  ],
  "keywords": [
    "egg",
    "egg-plugin",
    "egg-development",
    "development"
  ],
  "dependencies": {
    "bytes": "^3.0.0",
    "mz": "^2.7.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "egg": "^2.3.0",
    "egg-bin": "^4.4.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.18.1",
    "eslint-config-egg": "^7.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfile && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && egg-bin pkgfile --check && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "8, 10, 12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eggjs/egg-logview.git"
  },
  "bugs": {
    "url": "https://github.com/eggjs/egg/issues"
  },
  "homepage": "https://github.com/eggjs/egg-logview#readme",
  "author": "TZ <atian25@qq.com>",
  "license": "MIT"
}
