{
  "name": "egg-logview",
  "version": "1.0.0",
  "description": "Provide a log files viewer for egg development purpose.",
  "eggPlugin": {
    "name": "logview",
    "env": [
      "local",
      "default",
      "test",
      "unittest"
    ]
  },
  "files": [
    "app", "config", "lib", "app.js"
  ],
  "keywords": [
    "egg",
    "plugin",
    "logger",
    "development"
  ],
  "dependencies": {
    "bytes": "^2.4.0",
    "mz": "^2.6.0"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "egg": "^0.9.0",
    "egg-bin": "^2.0.2",
    "egg-ci": "^1.1.0",
    "egg-mock": "^2.3.1",
    "eslint": "^3.14.1",
    "eslint-config-egg": "^3.2.0",
    "supertest": "^2.0.1",
    "webstorm-disable-index": "^1.1.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "6, 7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eggjs/egg-logview.git"
  },
  "bugs": {
    "url": "https://github.com/eggjs/egg/issues"
  },
  "homepage": "https://github.com/eggjs/egg-logview#readme",
  "author": "",
  "license": "MIT",
  "boilerplate": {
    "name": "egg-boilerplate-plugin",
    "version": "1.5.2",
    "description": "boilerplate for egg plugin",
    "repository": {
      "type": "git",
      "url": "git@github.com:eggjs/egg-boilerplate-plugin.git"
    },
    "homepage": "https://github.com/eggjs/egg-boilerplate-plugin"
  }
}
